{
    "restartable": "rs",
    "verbose": true,
    "execMap": {
        "ts": "ts-node"
    },
    "watch": [
        "src/"
    ],
    "ext": "ts json",
    "env": {
        "NODE_ENV": "development"
    },
    "ignore": [
        "node_modules",
        "reports",
        "cache",
        "dist",
        "test",
        ".git"
    ]
}